{
    "name": "btp/composer-packages",
    "description": "Composer packages for unit testing",
    "keywords": ["phpunit"],
    "license": "GNU GPL v2",
    "require": {
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpunit/dbunit": ">=1.2",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php",
            "tests/AbstractDatabaseTestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
